{
    "name": "jminayat/modules-laravel",
    "description": "modular laravel applications",
    "keywords": [
        "jminayat",
        "laravel",
        "modules",
        "modular"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Javier Minaya Trinidad",
            "email": "javiermt05@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.0",
        "chumper/zipper": "1.0.x"
    },
    "autoload": {
        "psr-4": {
            "JMinayaT\\Modules\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JMinayaT\\Modules\\ModulesServiceProvider"
            ],
            "aliases": {
                "Module": "JMinayaT\\Modules\\Facades\\Module"
              }
         }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
